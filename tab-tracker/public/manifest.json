{
  "manifest_version": 3,
  "name": "Tab Usage Tracker",
  "version": "1.0",
  "description": "An extension to track your daily tab usage and time spent.",
  "permissions": [
    "storage",
    "tabs",
    "idle"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}